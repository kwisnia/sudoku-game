<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane fx:id="root" prefHeight="620.0" prefWidth="860.0" style="-fx-background-color: #F2DA8A;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.comp.view.PrimaryController">
   <children>
      <VBox alignment="CENTER" prefHeight="620.0" prefWidth="860.0" spacing="20.0" stylesheets="@primaryScreen.css">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="80.0" />
         </padding>
         <ChoiceBox fx:id="difficultyChoiceBox" prefWidth="150.0" style="-fx-pref-width: 150; -fx-background-color: #FFFFFF;" stylesheets="@primaryScreen.css" />
         <Button fx:id="primaryButton" onAction="#switchToSecondary" prefHeight="26.0" prefWidth="68.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #000000;" text="%startButtonLabel" />
         <Button fx:id="plButton" mnemonicParsing="false" onAction="#loadPL" prefHeight="26.0" prefWidth="37.0" text="PL" />
         <Button fx:id="engButton" mnemonicParsing="false" onAction="#loadEng" text="ENG" />
         <Button fx:id="frButton" mnemonicParsing="false" onAction="#loadFR" text="FR" />
      </VBox>
      <Label prefHeight="174.0" prefWidth="860.0" text="Sudoku Game" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <padding>
            <Insets left="345.0" top="130.0" />
         </padding>
      </Label>
   </children>
</Pane>
